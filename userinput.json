{
  "ProjectName":"TestProject",
  "ProjectDB":"MongoDB",
  "DBurl":"mongodb://127.0.0.1:27017/myDB",
  "Entities":[
    {
      "name":"user",
      "properties":[
        {
          "name":"username",
          "type":"String"
        },
        {
          "name":"password",
          "type":"String"
        },
        {
          "name":"uuid",
          "type":"String"
        }
      ],
      "index":["_id"]
    },
    {
      "name":"attack",
      "properties":[
        {
          "name":"timestamp",
          "type":"Date"
        },
        {
          "name":"place",
          "type":"Enum_String",
          "enum":["indoor","outdoor"]
        },
        {
          "name":"userId",
          "type":"Ref_ObjectId",
          "ref":"user._id"
        }
      ],
      "index":["_id"]
    }
  ]
}